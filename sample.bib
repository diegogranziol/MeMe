@Article{nerioptions,
author="Neri, Cassio
and Schneider, Lorenz",
title="Maximum entropy distributions inferred from option portfolios on an asset",
journal="Finance and Stochastics",
year="2012",
volume="16",
number="2",
pages="293--318",
